{
  "presets": [
    "env",
    "flow",
    "react"
  ],
  "plugins": [
      // enable styleName="" in react components
      ["react-css-modules"],
      // enable class properties
      ["transform-class-properties"],
      // async
      ["transform-regenerator"]
  ],
  "env": {
    "server": {
      "plugins": [
        [
          "react-css-modules",
          {
            // Don't import during SSR
            "removeImport": true
          }
        ]
      ]
    }
  }
}