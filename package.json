{
  "name": "4temps-tournament",
  "version": "0.0.1",
  "description": "A website to host 4temps tournaments",
  "main": "server/index.js",
  "repository": "git@github.com:SimonKinds/4temps-tournament.git",
  "author": "Simon Kindstr√∂m <simon.kindstrom@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "npm-run-all -p watch:client watch:server",
    "watch:server": "BABEL_ENV=server nodemon src/index.js --exec babel-node",
    "watch:client": "BABEL_ENV=client webpack --config config/webpack.dev.js",
    "build": "npm-run-all -p build:*",
    "build:server": "BABEL_ENV=server babel -s -d lib/ src/ ",
    "build:client": "BABEL_ENV=client webpack --config config/webpack.prod.js",
    "start:prod": "node lib/index.js",
    "test": "jest src/ -w=50%",
    "coverage": "jest src/ -w=50% --coverage",
    "test:watch": "jest src/ --watch",
    "lint": "npm-run-all -p lint:eslint lint:flow",
    "lint:fix": "prettier --write 'src/**/*.js' && eslint 'src/**/*.js' --fix",
    "lint:eslint": "eslint src/**",
    "lint:flow": "flow check",
    "validate": "npm-run-all -p lint test",
    "flow": "flow"
  },
  "engines": {
    "node": "10.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "bson-objectid": "^1.2.3",
    "compression": "^1.7.3",
    "connect-mongo": "^3.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.13.0",
    "js-yaml": "^3.14.0",
    "moment": "^2.22.2",
    "mongodb": "^3.1.4",
    "mongoose": "^4.13.15",
    "normalizr": "^3.2.4",
    "react": "^16.4.2",
    "react-datepicker": "^0.64.0",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-pack": "^0.1.5",
    "semantic-ui-react": "^0.77.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "flow-bin": "^0.61.0",
    "jest": "^21.2.1",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^3.12.0",
    "webpack-merge": "^4.1.4"
  }
}
