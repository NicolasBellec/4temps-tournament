{
  "name": "4temps-tournament",
  "version": "0.0.1",
  "description": "A website to host 4temps tournaments",
  "main": "server/index.js",
  "repository": "git@github.com:SimonKinds/4temps-tournament.git",
  "author": "Simon Kindstr√∂m <simon.kindstrom@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "npm-run-all -p watch:client watch:server",
    "watch:server": "BABEL_ENV=server nodemon src/index.js --exec babel-node",
    "watch:client": "BABEL_ENV=client webpack --config config/webpack.dev.js",
    "build": "npm-run-all -p build:*",
    "build:server": "BABEL_ENV=server babel -s -d lib/ src/ ",
    "build:client": "BABEL_ENV=client webpack --config config/webpack.prod.js",
    "start:prod": "node lib/index.js",
    "test": "jest --config jest.config.js src/ -w=50%",
    "coverage": "jest src/ -w=50% --coverage",
    "test:watch": "jest src/ --watch",
    "lint": "npm-run-all -p lint:eslint lint:flow",
    "lint:fix": "prettier --write 'src/**/*.js' && eslint 'src/**/*.js' --fix",
    "lint:eslint": "eslint src/**",
    "lint:flow": "flow check src/",
    "validate": "npm-run-all -p lint test",
    "flow": "flow"
  },
  "engines": {
    "node": "10.23.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bson-objectid": "^1.3.1",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "js-yaml": "^3.14.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.11",
    "normalizr": "^3.6.1",
    "react": "^16.14.0",
    "react-datepicker": "^0.64.0",
    "react-dom": "^16.14.0",
    "react-redux": "^5.1.2",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-pack": "^0.1.5",
    "semantic-ui-react": "^2.0.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "ajv": "^6.12.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "flow-bin": "^0.137.0",
    "jest": "^21.2.1",
    "mini-css-extract-plugin": "^1.2.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "style-loader": "^0.19.1",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-merge": "^4.2.2"
  }
}
