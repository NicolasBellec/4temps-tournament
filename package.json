{
  "name": "4temps-tournament",
  "version": "0.0.1",
  "description": "A website to host 4temps tournaments",
  "main": "server/index.js",
  "repository": "git@github.com:SimonKinds/4temps-tournament.git",
  "author": "Simon Kindstr√∂m <simon.kindstrom@gmail.com>",
  "license": "GPL-3.0",
  "scripts": {
    "start": "npm-run-all -p watch:client watch:server",
    "watch:server": "BABEL_ENV=server nodemon src/index.js --exec babel-node",
    "watch:client": "BABEL_ENV=client webpack --config config/webpack.dev.js",
    "build": "npm-run-all -p build:*",
    "build:server": "BABEL_ENV=server babel -s -d lib/ src/ ",
    "build:client": "BABEL_ENV=client webpack --config config/webpack.prod.js",
    "start:prod": "node lib/index.js",
    "test": "jest --config jest.config.js src/ -w=50%",
    "coverage": "jest --config jest.config.js src/ -w=50% --coverage",
    "test:watch": "jest src/ --watch",
    "lint": "npm-run-all -p lint:eslint lint:flow",
    "lint:fix": "prettier --write 'src/**/*.js' && eslint 'src/**/*.js' --fix",
    "lint:eslint": "eslint src/",
    "lint:flow": "flow check",
    "validate": "npm-run-all -p lint test",
    "flow": "flow"
  },
  "engines": {
    "node": "10.23.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bson-objectid": "^1.3.1",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.2.0",
    "js-yaml": "^3.14.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.11",
    "normalizr": "^3.6.1",
    "react": "^17.0.1",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-pack": "^0.1.5",
    "semantic-ui-react": "^2.0.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-flow": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@stryker-mutator/core": "^4.1.0",
    "@stryker-mutator/jest-runner": "^4.1.0",
    "ajv": "^6.12.6",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "chokidar": "^3.4.3",
    "css-loader": "^5.0.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "flow-bin": "^0.137.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.2.1",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "prettier": "^2.1.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-merge": "^5.3.0"
  }
}
